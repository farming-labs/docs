{
  "name": "@farming-labs/next",
  "version": "0.0.2-beta.1",
  "description": "Next.js adapter for @farming-labs/docs â€” MDX config wrapper",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.mts",
      "import": "./dist/config.mjs",
      "default": "./dist/config.mjs"
    }
  },
  "files": ["dist"],
  "keywords": ["docs", "next", "mdx", "documentation"],
  "author": "Farming Labs",
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^16.1.6",
    "fumadocs-core": "^16.6.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
