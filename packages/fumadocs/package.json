{
  "name": "@farming-labs/theme",
  "version": "0.0.3-beta.1",
  "description": "Theme package for @farming-labs/docs â€” layout, provider, MDX components, and styles",
  "keywords": [
    "docs",
    "documentation",
    "fumadocs",
    "theme"
  ],
  "license": "MIT",
  "author": "Farming Labs",
  "files": [
    "dist",
    "styles"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./mdx": {
      "types": "./dist/mdx.d.mts",
      "import": "./dist/mdx.mjs",
      "default": "./dist/mdx.mjs"
    },
    "./default": {
      "types": "./dist/default/index.d.mts",
      "import": "./dist/default/index.mjs",
      "default": "./dist/default/index.mjs"
    },
    "./darksharp": {
      "types": "./dist/darksharp/index.d.mts",
      "import": "./dist/darksharp/index.mjs",
      "default": "./dist/darksharp/index.mjs"
    },
    "./pixel-border": {
      "types": "./dist/pixel-border/index.d.mts",
      "import": "./dist/pixel-border/index.mjs",
      "default": "./dist/pixel-border/index.mjs"
    },
    "./colorful": {
      "types": "./dist/colorful/index.d.mts",
      "import": "./dist/colorful/index.mjs",
      "default": "./dist/colorful/index.mjs"
    },
    "./shiny": {
      "types": "./dist/shiny/index.d.mts",
      "import": "./dist/shiny/index.mjs",
      "default": "./dist/shiny/index.mjs"
    },
    "./darkbold": {
      "types": "./dist/darkbold/index.d.mts",
      "import": "./dist/darkbold/index.mjs",
      "default": "./dist/darkbold/index.mjs"
    },
    "./greentree": {
      "types": "./dist/greentree/index.d.mts",
      "import": "./dist/greentree/index.mjs",
      "default": "./dist/greentree/index.mjs"
    },
    "./search": {
      "types": "./dist/search.d.mts",
      "import": "./dist/search.mjs",
      "default": "./dist/search.mjs"
    },
    "./api": {
      "types": "./dist/docs-api.d.mts",
      "import": "./dist/docs-api.mjs",
      "default": "./dist/docs-api.mjs"
    },
    "./css": "./styles/default.css",
    "./base/css": "./styles/base.css",
    "./default/css": "./styles/default.css",
    "./darksharp/css": "./styles/darksharp.css",
    "./pixel-border/css": "./styles/pixel-border.css",
    "./colorful/css": "./styles/colorful.css",
    "./shiny/css": "./styles/shiny.css",
    "./darkbold/css": "./styles/darkbold.css",
    "./greentree/css": "./styles/greentree.css",
    "./presets/neutral": "./styles/presets/neutral.css",
    "./presets/black": "./styles/presets/black.css",
    "./presets/base": "./styles/presets/base.css"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fumadocs-core": "^16.6.1",
    "fumadocs-ui": "^16.6.1",
    "gray-matter": "^4.0.3",
    "sugar-high": "^0.9.5"
  },
  "devDependencies": {
    "@farming-labs/docs": "workspace:*",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "next": ">=14.0.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@farming-labs/docs": ">=0.0.1",
    "next": ">=16.0.0",
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  }
}
