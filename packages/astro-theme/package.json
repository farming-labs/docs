{
  "name": "@farming-labs/astro-theme",
  "version": "0.0.4",
  "description": "Astro UI components for @farming-labs/docs â€” layout, sidebar, TOC, search, and theme toggle",
  "keywords": [
    "astro",
    "docs",
    "documentation",
    "theme"
  ],
  "license": "MIT",
  "author": "Farming Labs",
  "files": [
    "src",
    "styles"
  ],
  "type": "module",
  "typesVersions": {
    "*": {
      "pixel-border": [
        "./src/themes/pixel-border.d.ts"
      ],
      "darksharp": [
        "./src/themes/darksharp.d.ts"
      ],
      "fumadocs": [
        "./src/themes/default.d.ts"
      ],
      ".": [
        "./src/index.d.ts"
      ],
      "greentree": [
        "./src/themes/greentree.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./src/components/*": "./src/components/*",
    "./fumadocs": {
      "types": "./src/themes/default.d.ts",
      "import": "./src/themes/default.js",
      "default": "./src/themes/default.js"
    },
    "./pixel-border": {
      "types": "./src/themes/pixel-border.d.ts",
      "import": "./src/themes/pixel-border.js",
      "default": "./src/themes/pixel-border.js"
    },
    "./darksharp": {
      "types": "./src/themes/darksharp.d.ts",
      "import": "./src/themes/darksharp.js",
      "default": "./src/themes/darksharp.js"
    },
    "./colorful": {
      "types": "./src/themes/colorful.d.ts",
      "import": "./src/themes/colorful.js",
      "default": "./src/themes/colorful.js"
    },
    "./css": "./styles/docs.css",
    "./fumadocs/css": "./styles/docs.css",
    "./styles/pixel-border.css": "./styles/pixel-border.css",
    "./styles/darksharp.css": "./styles/darksharp.css",
    "./pixel-border/css": "./styles/pixel-border-bundle.css",
    "./darksharp/css": "./styles/darksharp-bundle.css",
    "./styles/colorful.css": "./styles/colorful.css",
    "./colorful/css": "./styles/colorful-bundle.css",
    "./greentree": {
      "types": "./src/themes/greentree.d.ts",
      "import": "./src/themes/greentree.js",
      "default": "./src/themes/greentree.js"
    },
    "./styles/greentree.css": "./styles/greentree.css",
    "./greentree/css": "./styles/greentree-bundle.css"
  },
  "scripts": {
    "build": "echo 'Astro components are shipped as source'",
    "typecheck": "echo 'ok'"
  },
  "dependencies": {
    "@farming-labs/astro": "workspace:*",
    "@farming-labs/docs": "workspace:*",
    "sugar-high": "^0.9.5"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  }
}